<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Hello Newstore!</title>
  <link rel="stylesheet" href="<%= static_path(@conn, "/css/app.css") %>">
</head>

<body style="margin: 0;">
  <!-- Grid start -->
  <div class="sq-page">
    <!--NavBar start-->
    <nav class="sq-nav-area">
      <div class="sq-navbar-layout">
        <!--NavBar part 1/3-->
        <div class="sq-navbar-nav sq-secondary sq-text-right">
          <!-- Links -->
          <div class=" sq-flex sq-air-x-05 sq-justify-evenly">

          <a href="#" class="sq-text-white sq-secondary  sq-x-padding-05">Sign up for our newsletter</a>
          <a href="#" class="sq-text-white sq-secondary  sq-x-padding-05">Welcome to out Store!</a>
          <a href="#" class="sq-text-white sq-secondary ">Contact us</a>
          </div>
        </div>
        <!--NavBar part 2/3-->
        <div class="sq-navbar-domain sq-secondary sq-flex" style="width:100%;">
          <a href="/" class="sq-text-white sq-secondary">Home!</a>
          <div class="sq-flex-1"></div>
          <!-- SearchBar -->
          <div class="sq-flex-auto sq-form">
            <input type="text" placeholder="Search..." name="search" class="sq-air-r-05 ">
          </div>
          <!--SearchBar end-->
          <!--SignIn button -->
          <div class="sq-flex-1 sq-text-right"></div>
          <!--SignIn button end -->
          <!--ShoppingCart button -->

          <!--ShoppingCart button end -->
        </div>
        <!-- Navbar part 3/3-->
        <div class="sq-navbar-links sq-secondary-highlight sq-text-center">
          <div class="sq-nav-btn-group sq-flex sq-justify-evenly">
            <a href="/categories">Categories</a>
            <a href="/Products">Products</a>
            <a href="Products">Best selgere</a>
            <a href="Products">Kampanje</a>
            <button class="sq-nav-dropdown" style="position:absolute; right:0">
              <svg id="i-cart" viewBox="0 0 32 32" width="20" height="20" fill="none" stroke="currentcolor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2">
                <path d="M6 6 L30 6 27 19 9 19 M27 23 L10 23 5 2 2 2" />
                <circle cx="25" cy="27" r="2" />
                <circle cx="12" cy="27" r="2" />
              </svg>
              <div class=" sq-secondary sq-nav-dropdown-content-right">
                <div class="sq-card">
                  <div class="sq-flex">
                    <img class="sq-border-1" style="min-width:75px; max-width:75px; max-height:75px; min-height:75px;"></img>
                    <h3>Pris:</h3>
                    <h3>a hundred million dollars</h3>
                  </div>
                  <h3>This</h3>
                  <h3>is</h3>
                  <h3>a</h3>
                  <h3>Test</h3>
                  <p  >Go to shopping cart</p>
                </div>
              </div>
              </button>

          </div>
      </div>

    </nav>
    <!--NavBar end-->
    <!-- Main content start -->
    <main class="sq-main">
      <%= render @view_module, @view_template, assigns %>
    </main>
    <!-- Main content end -->
    <!-- Footer start -->

    <!-- Footer end -->
  </div>
  <!-- Grid end-->
  <script src="<%= static_path(@conn, "/js/app.js") %>"></script>
</body>
</html>
