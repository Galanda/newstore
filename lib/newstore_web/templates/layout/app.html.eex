<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Hello Newstore!</title>
  <link rel="stylesheet" href="<%= static_path(@conn, "/css/app.css") %>">
</head>

<body class="sq-viewport" style="margin: 0;">
  <div class="sq-page">
    <!--NavBar part 1/3-->
    <nav class="sq-nav-area">
      <div class="sq-navbar-layout">
        <div class="sq-navbar-nav sq-secondary sq-text-right">
          <!-- Links -->
          <div class=" sq-flex sq-air-x-05 sq-justify-evenly">
          
          <a href="#" class="sq-text-white sq-secondary  sq-x-padding-05">Sign up for our newsletter</a>
          <a href="#" class="sq-text-white sq-secondary  sq-x-padding-05">Welcome to out Store!</a>
          <a href="#" class="sq-text-white sq-secondary ">Contact us</a>
          </div>
        </div> 
        <!--NavBar part 2/3-->
        <div class="sq-navbar-domain sq-secondary sq-flex" style="width:100%;">
          <a href="/" class="sq-text-white sq-secondary">Home!</a>
          <div class="sq-flex-1"></div>
          <!-- SearchBar -->
          <div class="sq-flex-auto sq-form">
            <input type="text" placeholder="Search..." name="search" class="sq-air-r-05 ">
          </div>
          <!--SearchBar end-->
          <!--SignIn button -->
          <div class="sq-flex-1 sq-text-right"></div>
          <!--SignIn button end -->
          <!--ShoppingCart button -->
          <div></div>
          <!--ShoppingCart button end -->
        </div>
        <!-- Navbar part 3/3-->
        <div class="sq-navbar-links sq-secondary-highlight sq-text-center">
          <div class="sq-nav-btn-group">
            <a href="#">Categories</a>
            <a href="/products">Products</a>
            <a href="#">Best selgere</a>
            <a href="#">Kampanje</a>
          </div>
        </div>
      </div>  
    </nav>




    




    <main role="main sq-main">
      <%= render @view_module, @view_template, assigns %>
    </main>
    <footer class="sq-secondary sq-footer">
      <div class="sq-secondary sq-flex sq-flex-row sq-justify-around">
          <div>
           Phone: <a href="tel:0000" class="sq-text-white sq-secondary">#########</a>
          </div>
          <div>
          <a href="/about" class="sq-text-white sq-secondary">About</a>
          </div>
          <div>
          <a href="/support" class="sq-text-white sq-secondary">Support</a>
          </div>
          <div>
          <a href="/faq" class="sq-text-white sq-secondary">FAQ</a>
          </div>
          <div>
          <a href="mailto:nobody@online.no" class="sq-text-white sq-secondary">Mail</a>
          </div>
      </div>    
    </footer>
  </div>
  <!-- /container -->
  <script src="<%= static_path(@conn, "/js/app.js") %>"></script>
</body>

</html>
